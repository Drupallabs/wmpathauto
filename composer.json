{
    "name": "wieni/wmpathauto",
    "type": "drupal-module",
    "description": "Dynamic path aliases, pathauto patterns & automatic updating of aliases",
    "license": "MIT",
    "authors": [
        {
            "name": "Wieni",
            "email": "info@wieni.be"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "drupal/core": "^8.0 || ^9.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.0",
        "wieni/wmcodestyle": "^1.0"
    },
    "scripts": {
        "coding-standards": [
            "@composer normalize",
            "php-cs-fixer fix --config=.php_cs.php"
        ]
    }
}
